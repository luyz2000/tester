!!!
%html
	%head
		%meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
		%title Proyecto
		= csrf_meta_tags
		= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload'
		%link{"data-turbolinks-track" => "reload", :href => "https://maxcdn.bootstrapcdn.com/bootswatch/3.3.7/cerulean/bootstrap.min.css", :rel => "stylesheet"}/
		= javascript_include_tag 'application', 'data-turbolinks-track': 'false'
	%body
		%p.notice= notice
		%p.alert= alert
		=render "layouts/navegador"
		.container
			= yield
